{
  "rules": {
    "no-process-exit": 2,
    "max-len": [
      2,
      100,
      2
    ],
    "no-new-require": 2,
    "space-in-brackets": [
      2,
      "never"
    ],
    "wrap-iife": [
      2,
      "outside"
    ],
    "no-self-compare": 2,
    "no-unused-expressions": 0,
    "no-warning-comments": 2,
    "max-params": [
      2,
      5
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "no-else-return": 2,
    "max-depth": 2,
    "max-nested-callbacks": [
      2,
      5
    ],
    "block-scoped-var": 2,
    "no-lonely-if": 2,
    "handle-callback-err": 2,
    "no-extra-parens": 2,
    "no-nested-ternary": 2,
    "brace-style": [
      2,
      "1tbs"
    ],
    "no-path-concat": 2,
    "no-eq-null": 2,
    "no-use-before-define": "nofunc",
    "func-style": [
      2,
      "declaration"
    ],
    "semi": [
      2,
      "always"
    ],
    "space-unary-word-ops": 2,
    "valid-jsdoc": 0,
    "strict": 0,
    "complexity": [1, 10],
    "no-mixed-requires": [
      2,
      true
    ],
    "no-underscore-dangle": 0,
    "space-after-keywords": [
      2,
      "always"
    ],
    "curly": 0
  },
  "env": {
    "node": true,
    "mocha": true
  }
}%